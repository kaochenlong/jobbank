<h1>登入</h1>

<%= form_with(model: @user, url: sessions_path) do |f| %>

  <div class="field">
    <%= f.label :email, 'Email 信箱' %>
    <%= f.email_field :email%>
  </div>

  <div class="field">
    <%= f.label :password, '密碼' %>
    <%= f.password_field :password %>
  </div>

  <%= f.submit '登入' %>

<% end %>
